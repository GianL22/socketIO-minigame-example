.modal {
    width: 70%;
    max-height: 80%;
    background-color: rgb(11, 224, 18);
    border-top-left-radius: 32px;
    border-top-right-radius: 32px;
    border-bottom-right-radius: 32px;
    border-bottom-left-radius: 32px;
    padding-top: 32px;
    padding-right: 32px;
    padding-bottom: 32px;
    padding-left: 32px;
    transition-duration: 0.4s;
    transition-timing-function: ease-in-out-back;
}

.container {
    align-items: center;
    justify-content: center;
    background-color: rgba(0, 0, 0, 0.25);
}

.modal-header {
    height: 10%;
}

.modal-body {
    height: 70%;
}

.modal-bottom {
    height: 20%;
    background-color: rgba(0, 0, 0, 0.44);
    border-top-left-radius: 32px;
    border-top-right-radius: 32px;
    border-bottom-right-radius: 32px;
    border-bottom-left-radius: 32px;
    align-items: center;
}

.open-case-title {
}

.open-case-title Label {
    color: rgb(255, 255, 255);
    -unity-font-definition: url('project://database/Assets/UI/common/font/LTSaeada-Medium%20SDF.asset?fileID=11400000&guid=38730d8fb4d14ab4ba8404460129a0ce&type=2#LTSaeada-Medium SDF');
    font-size: 256%;
    -unity-text-align: upper-center;
}

.open-case {
    height: 80%;
    align-items: center;
}

.open-case-message {
    height: 20%;
    align-items: center;
    justify-content: center;
}

.case-container {
    width: 30%;
    max-height: 90%;
}

.case {
    width: 100%;
    height: 100%;
    transition-duration: 0.1s;
}

.open-case-message Label {
    -unity-font-definition: url('project://database/Assets/UI/common/font/LTSaeada-Medium%20SDF.asset?fileID=11400000&guid=38730d8fb4d14ab4ba8404460129a0ce&type=2#LTSaeada-Medium SDF');
    font-size: 256%;
    color: rgb(255, 255, 255);
}

.continue-message {
    justify-content: center;
}

.continue-message Label {
    font-size: 256%;
    -unity-font-definition: url('project://database/Assets/UI/common/font/LTSaeada-Medium%20SDF.asset?fileID=11400000&guid=38730d8fb4d14ab4ba8404460129a0ce&type=2#LTSaeada-Medium SDF');
    color: rgb(255, 255, 255);
}

.case-container-initial {
}

.safed-message {
    align-items: center;
    justify-content: center;
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out-back;
}

.safed-message Label {
    -unity-font-definition: url('project://database/Assets/UI/common/font/LTSaeada-Medium%20SDF.asset?fileID=11400000&guid=38730d8fb4d14ab4ba8404460129a0ce&type=2#LTSaeada-Medium SDF');
    color: rgb(255, 255, 255);
    -unity-text-align: upper-center;
    font-size: 512%;
    background-color: rgb(24, 170, 29);
    border-left-color: rgb(24, 170, 29);
    border-right-color: rgb(24, 170, 29);
    border-top-color: rgb(24, 170, 29);
    border-bottom-color: rgb(24, 170, 29);
    border-top-width: 32px;
    border-right-width: 32px;
    border-bottom-width: 32px;
    border-left-width: 32px;
    border-top-left-radius: 32px;
    border-top-right-radius: 32px;
    border-bottom-right-radius: 32px;
    border-bottom-left-radius: 32px;
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out-back;
}

.eliminated-message {
    align-items: center;
    justify-content: center;
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out-back;
}

